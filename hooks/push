#!/bin/bash


echo BUILD_CODE $BUILD_CODE
echo DOCKER_REPO $DOCKER_REPO
echo DOCKER_TAG $DOCKER_TAG
echo SOURCE_TYPE $SOURCE_TYPE
echo SOURCE_URL $SOURCE_URL
echo SOURCE_BRANCH $SOURCE_BRANCH
echo SOURCE_COMMIT $SOURCE_COMMIT
echo SSH_PRIVATE $SSH_PRIVATE
echo BUILD_PATH $BUILD_PATH
echo DOCKERFILE_PATH $DOCKERFILE_PATH
echo DOCKER_HOST $DOCKER_HOST
echo DOCKERCFG $DOCKERCFG
echo CACHE_TAG $CACHE_TAG
echo SIGNED_URLS $SIGNED_URLS
echo BYON $BYON
echo PUSH $PUSH
echo PUSH_ATTEMPT_COUNT $PUSH_ATTEMPT_COUNT
echo LOG_BUILD_STEPS $LOG_BUILD_STEPS
echo MAX_LOG_SIZE $MAX_LOG_SIZE
echo IMAGE_NAME $IMAGE_NAME

echo "-- env --"
env
echo "-- set --"
set
echo "-- end --"

make push-all
